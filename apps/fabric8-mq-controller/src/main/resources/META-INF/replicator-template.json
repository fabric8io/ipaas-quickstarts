{
  "id": "fabric8MQ-sharding-controller",
  "kind": "ReplicationController",
  "apiVersion": "v1beta2",
  "desiredState": {
    "replicas": 1,
    "replicaSelector": {
      "container": "java",
      "component": "fabric8MQ",
      "provider": "fabric8",
      "group": "fabric8MQGroup"
    },
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta2",
          "id": "fabric8-mqPod",
          "containers": [
            {
              "name": "fabric8-mq-container",
              "image": "dockerhost:5000/fabric8/fabric8-mq:2.2-SNAPSHOT",
              "imagePullPolicy": "PullAlways",
              "env": [
              ],
              "ports": [
                {
                  "containerPort": 6162

                },
                {
                  "containerPort": 8778

                }
              ]
            }
          ]
        }
      },
      "labels": {
        "container": "java",
        "component": "fabric8MQ",
        "provider": "fabric8",
        "group": "fabric8MQGroup"
      }
    }
  },
  "labels": {
    "container": "java",
    "component": "fabric8MQ",
    "provider": "fabric8",
    "group": "fabric8MQGroup"
  }
}
